dn:  olcDatabase={1}mdb,cn=config
changetype: modify
replace: olcSuffix
olcSuffix: ${dn}
-
replace: olcRootDN
olcRootDN: ${dn}
-
replace: olcRootPW
olcRootPW: ${password}
-
replace: olcAccess
olcAccess: {0}to *
  by dn.base="gidNumber=0+uidNumber=0,cn=peercred,cn=external,cn=auth" write
  by anonymous auth
  by * read